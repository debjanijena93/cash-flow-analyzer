{
  "name": "cashflowanalyzer",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1.9.1",
    "@sap/cds": "^8",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/xsenv": "^5.6.1",
    "@sap/xssec": "^4.8.0",
    "csv-parser": "^3.2.0",
    "express": "^4",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "debug": "cds watch",
    "watch-cashflowanalyzerapp": "cds watch --open cashflowanalyzerapp/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy cashflowanalyzer --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "destinations": true,
      "auth": {
        "[production]": {
          "kind": "xsuaa"
        },
        "[development]": {
          "kind": "mocked"
        }
      },
      "db": {
        "[production]": {
          "kind": "hana"
        },
        "[development]": {
          "kind": "sqlite"
        }
      }
    }
  },
  "sapux": [
    "app/cashflowanalyzerapp"
  ]
}
